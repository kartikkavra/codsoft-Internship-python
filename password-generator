import random
length = int(input("LENGTH "))
letters = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
numbers = "0123456789"
symbols = "!@#$%^&*()_+-="
char = letters + numbers + symbols
password = ""
for i in range(length):
    password += random.choice(char)
print("Your new password is:", password)

